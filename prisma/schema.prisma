generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model checkmate {
  game String @id
}

model initial {
  moves    String    @id(map: "initial_pk")
  progress String?   @unique(map: "initial_progress_uindex")
  length   Int?      @default(4) @db.SmallInt
  time     DateTime? @default(now()) @db.Timestamp(6)

  @@index([time], map: "initial_time_index")
}
