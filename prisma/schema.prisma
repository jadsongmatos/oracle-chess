generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model checkmate {
  game String @id
}

model initial {
  moves    String    @id(map: "4_pk") @unique(map: "4_moves_uindex")
  progress String?   @unique(map: "4_progress_uindex")
  length   Int?      @default(4) @db.SmallInt
  time     DateTime? @default(now()) @db.Timestamp(6)
}
